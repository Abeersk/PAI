<svg width="1200" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a2a6c" />
      <stop offset="100%" stop-color="#2a4d69" />
    </linearGradient>
  </defs>
  <rect width="100%" height="100%" fill="url(#bgGradient)" />

  <!-- Robot illustration -->
  <g transform="translate(600, 300)">
    <!-- Robot body -->
    <rect x="-60" y="-30" width="120" height="90" rx="15" fill="#f0f0f0" stroke="#25c2a0" stroke-width="3"/>

    <!-- Robot head -->
    <circle cx="0" cy="-60" r="35" fill="#f0f0f0" stroke="#25c2a0" stroke-width="3"/>

    <!-- Robot eyes -->
    <circle cx="-15" cy="-67" r="6" fill="#25c2a0"/>
    <circle cx="15" cy="-67" r="6" fill="#25c2a0"/>

    <!-- Robot mouth -->
    <rect x="-15" y="-50" width="30" height="5" rx="2" fill="#25c2a0"/>

    <!-- Robot arms -->
    <line x1="-75" y1="0" x2="-120" y2="30" stroke="#f0f0f0" stroke-width="9" stroke-linecap="round"/>
    <line x1="75" y1="0" x2="120" y2="30" stroke="#f0f0f0" stroke-width="9" stroke-linecap="round"/>

    <!-- Robot legs -->
    <line x1="-30" y1="60" x2="-30" y2="105" stroke="#f0f0f0" stroke-width="9" stroke-linecap="round"/>
    <line x1="30" y1="60" x2="30" y2="105" stroke="#f0f0f0" stroke-width="9" stroke-linecap="round"/>

    <!-- Circuit pattern -->
    <g stroke="#25c2a0" stroke-width="1.5" fill="none">
      <circle cx="0" cy="15" r="22" stroke-dasharray="4,4"/>
      <circle cx="0" cy="15" r="37" stroke-dasharray="4,4"/>
      <path d="M-22,15 L-37,0 M37,0 L22,15 M0,-7 L0,-22" stroke-dasharray="3,3"/>
    </g>
  </g>

  <!-- Floating geometric shapes -->
  <circle cx="150" cy="120" r="22" fill="rgba(37, 194, 160, 0.3)" />
  <circle cx="1050" cy="180" r="30" fill="rgba(37, 194, 160, 0.3)" />
  <rect x="975" y="450" width="37" height="37" fill="rgba(37, 194, 160, 0.3)" transform="rotate(45 993.5 468.5)" />
  <polygon points="225,525 255,495 285,525 255,555" fill="rgba(37, 194, 160, 0.3)" />

  <!-- Additional floating elements -->
  <path d="M400,100 Q450,50 500,100 T600,100" stroke="rgba(37, 194, 160, 0.4)" stroke-width="2" fill="none"/>
  <path d="M300,400 Q350,350 400,400 T500,400" stroke="rgba(37, 194, 160, 0.4)" stroke-width="2" fill="none"/>
</svg>